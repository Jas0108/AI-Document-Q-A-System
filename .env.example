# API Keys (Required)
GROQ_API_KEY=your_groq_api_key_here

# Embedding Configuration (Optional)
# Set to "google" to use Google Generative AI (requires GOOGLE_API_KEY)
# Set to "huggingface" to use free local embeddings (default, no API key needed)
EMBEDDING_PROVIDER=huggingface

# Google API Key (Only needed if EMBEDDING_PROVIDER=google)
# GOOGLE_API_KEY=your_google_api_key_here

# Optional Configuration
# GROQ_MODEL=Llama3-8b-8192
# CHUNK_SIZE=1000
# CHUNK_OVERLAP=200
